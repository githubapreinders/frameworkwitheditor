<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="logindetails">
    <xs:complexType>
      <xs:sequence>
        <xs:element type="xs:string" name="accesstoken"/>
        
        <xs:element name="user">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:integer" name="id"/>
              <xs:element type="xs:string" minOccurs="0" name="firstname"/>
              <xs:element type="xs:string" minOccurs="0" name="lastname"/>
              <xs:element type="xs:string" name="instancename"/>
              <xs:element type="xs:string" name="role"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>